#: templates/froide_fax/_send_message_form.html:5
#: templates/froide_fax/_send_message_form.html:13
#: templates/froide_fax/_send_message_form.html:31
msgid "Send as fax"
msgstr "Als Fax versenden"

#: templates/froide_fax/_send_message_form.html:25
#, python-format
msgid ""
"\n"
"              Send fax to %(name)s with fax number %(fax_number)s.\n"
"            "
msgstr ""
"\n"
"              Fax an %(name)s senden mit der Faxnummer %(fax_number)s.\n"
"            "

#: templates/froide_fax/_signature_form.html:6
msgid "Save your signature"
msgstr "Speichern Sie Ihre Unterschrift"

#: templates/froide_fax/_signature_form.html:13
msgid ""
"\n"
"            Sometimes public authorities require a written letter with a "
"signature from you.\n"
"            We can send a letter as a fax with your signature, if you "
"provide it below.\n"
"          "
msgstr ""
"\n"
"            Manche Behörden verlangen schriftliche Nachrichten mit "
"Unterschrift von Ihnen.\n"
"            Wir können Ihre Nachricht als Fax mit Ihrer Unterschrift "
"versenden, wenn Sie Ihre Unterschrift hier eintragen.\n"
"          "

#: templates/froide_fax/_signature_form.html:20
msgid ""
"\n"
"            We will only place your signature under requests and messages "
"that you send.\n"
"          "
msgstr ""
"\n"
"            Wir werden Ihre Unterschrift ausschließlich unter Anfragen und "
"Nachrichten setzen, die wir in Ihrem Auftrag versenden.\n"
"          "

#: templates/froide_fax/_signature_form.html:25
msgid ""
"\n"
"            Please write your signature in the box below by using your mouse "
"on Desktop or your finger on touch screens.\n"
"          "
msgstr ""
"\n"
"            Bitte schreiben Sie Ihre Unterschrift in den Kasten, entweder "
"mit Ihrem gedrückten Mauszeiger auf Desktopgeräten oder mit Ihrem Finger auf "
"Mobilgeräten.\n"
"          "

#: templates/froide_fax/_signature_form.html:45
msgid "Save signature"
msgstr "Unterschrift speichern"

#: templates/froide_fax/_signature_form.html:52
msgid "Delete signature"
msgstr "Unterschrift löschen"

#: templates/froide_fax/message_letter.html:6
msgid "Via"
msgstr "Via"

#: templates/froide_fax/message_letter.html:7
msgid "Fax and email"
msgstr "Fax und E-Mail"

#: templates/froide_fax/message_letter.html:11
msgid "Kind Regards,"
msgstr "Mit freundlichen Grüßen"

#: templates/froide_fax/widgets/signature.html:15
msgid "Write your signature in the box above."
msgstr "Bitte schreiben Sie Ihre Unterschrift in den obigen Kasten."

#: templates/froide_fax/widgets/signature.html:20
msgid "Undo"
msgstr "Rückgängig"

#: templates/froide_fax/widgets/signature.html:23
msgid "Clear"
msgstr "Leeren"

#: views.py:108
msgid "Signature has been saved."
msgstr "Unterschrift wurde gespeichert."

#: views.py:112
msgid "Signature has been removed."
msgstr "Unterschrift wurde gelöscht."

